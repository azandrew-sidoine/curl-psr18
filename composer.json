{
    "name": "drewlabs/psr18",
    "description": "PSR18 compatible http client based on PHP curl client library",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": ">=7.0",
        "donatj/mock-webserver": "^2.6"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Drewlabs\\Psr18\\": "src/"
        }
    },
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "asmyns.platonnas29@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "psr/http-client": "^1.0",
        "drewlabs/psr7": "^0.1.5|^0.2.0",
        "drewlabs/curl-client": "^0.3.0|^0.4.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
